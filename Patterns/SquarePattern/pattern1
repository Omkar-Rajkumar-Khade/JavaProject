package Patterns_code;
import java.util.Scanner;
public class Basic_pattern {

	public static void main(String[] args) {
		Scanner s=new Scanner(System.in);
		System.out.print("Enter a number:");
		//here number of rows is equal to number of column and printing star
		int n=s.nextInt();
		int i=1;
		while(i<=n) {
			int j=1;
			while(j<=n) {
				System.out.print("*");
				j=j+1;
			}
			System.out.println();
			i=i+1;
		}

	}

}
